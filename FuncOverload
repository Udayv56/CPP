#include <iostream>
using namespace std;

//Function overloading
int add(int a,int b){

    return a+b;
    
}

int add(int a,int b,int c){

    return a+b+c;
    
}
int main()
{
    cout<<"The sum of 3 and 6 is "<<add(3,6)<<endl;
    cout<<"The sum of 3,6 and 7 is "<<add(3,6,7)<<endl;
    return 0;
}
